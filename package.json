{
  "name": "polyglot-key-demo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "build:android": "cd android && gradlew.bat assembleDebug",
    "build:ios": "cd ios && xcodebuild -workspace PolyglotKey.xcworkspace -scheme PolyglotKey -configuration Debug build",
    "lint": "expo lint",
    "lint:ts": "tsc --noEmit",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "detox test --configuration android.emu.debug",
    "test:android": "cd android && ./gradlew testDebugUnitTest",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild",
    "postinstall": "expo install --fix"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.3",
    "expo-camera": "~16.1.5",
    "expo-clipboard": "~7.1.4",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.3",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.0.2",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "franc": "^6.2.0",
    "langdetect": "^0.2.1",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/franc": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/langdetect": "^0.2.2",
    "@types/react": "~19.0.10",
    "@types/react-native-fs": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "audit-ci": "^7.1.0",
    "detox": "^20.22.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "expo-module-scripts": "^4.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "typescript": "~5.8.3"
  }
}
